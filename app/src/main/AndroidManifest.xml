<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mike.itesm.marti">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.mike.itesm.Activities.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mike.itesm.Activities.Admin.MainActivityAdmin" />
        <activity android:name="com.mike.itesm.Activities.Admin.newProduct" />
        <activity android:name="com.mike.itesm.Activities.Admin.newUser" />
        <activity android:name="com.mike.itesm.Activities.User.MainActivityUser"></activity>
    </application>

</manifest>